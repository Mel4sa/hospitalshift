{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "dotnet: build",
		"type": "dotnet",
		"command": "build",
		"args": [
		  "${workspaceFolder}"
		],
		"problemMatcher": [
		  "$msCompile"
		],
		"group": {
		  "kind": "build",
		  "isDefault": true
		}
	  },
	  {
		"label": "dotnet: run",
		"type": "dotnet",
		"command": "run",
		"args": [
		  "--urls",
		  "http://localhost:5000"
		],
		"group": "none",
		"problemMatcher": []
	  }
	]
  }