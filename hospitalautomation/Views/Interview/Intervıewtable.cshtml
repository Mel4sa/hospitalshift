@model List<hospitalautomation.Models.Dtos.UserDto>

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container mt-5">
    <div class="text-center mb-4">
        <h1 class="fw-bold text-primary" style="letter-spacing: 2px; text-transform: uppercase; font-family: 'Arial', sans-serif;">
            <i class="bi bi-calendar2-check"></i> Randevular
        </h1>
        <p class="text-muted">Planlanan tüm randevuları burada görüntüleyebilirsiniz</p>
    </div>

    <div class="table-responsive">
        <table class="table table-striped table-hover shadow-sm">
            <thead class="table-primary">
                <tr>
                    <th scope="col" class="text-center">Randevu Sahibi</th>
                    <th scope="col" class="text-center">Görüşeceği Öğretim Üyesi</th>
                    <th scope="col" class="text-center">Tarih</th>
                    <th scope="col" class="text-center">Randevu Durumu</th>
                </tr>
            </thead>
            <tbody>
               
            </tbody>
        </table>
    </div>
</div>